{
  "name": "wawa-push-server",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "db:create": "wrangler d1 create wawa-push-db",
    "db:migrate": "wrangler d1 execute wawa-push-db --file=schema.sql",
    "db:migrate:local": "wrangler d1 execute wawa-push-db --file=schema.sql --local"
  },
  "dependencies": {
    "hono": "^4.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.0.0",
    "wrangler": "^4.0.0",
    "typescript": "^5.0.0"
  }
}
